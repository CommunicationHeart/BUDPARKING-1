/* pages/parkingLotMap/parkingLotMap.wxss */
.page {
    font-size: 32rpx;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
    font-family: 微软雅黑;
}
.page_hd{
    padding: 10rpx 50rpx 10rpx 50rpx;
    text-align: center;
}
.page_title{
    display: inline-block;
    padding: 20rpx 40rpx;
    font-size: 32rpx;
    color: #AAAAAA;
    border-bottom: 1px solid #CCCCCC;
}
.section{
    margin-bottom: 20rpx;
}
.section_gap{
    padding: 0 00rpx;
}
.text-view{
    color: gray;
    font-size: 13px;
    padding-right: 5px;
    display: inline-block;
}
.money-text{
    color: glod;
    font-size: 27px;
    padding-right: 18px;
    display: inline-block;
}
.local-view{
    color: black;
    font-size: 27px;
    padding-right: 1px;
    display: inline-block;
}
.Timer-view{
    color: black;
    font-size: 13px;
    padding-right: 18px;
    display: inline-block;   
    position: fiexd;
    bottom: 0;  
}
.clock_group{
    display:flex;
    padding-left: 18rpx;
}
.clock{
    height: 30rpx;
    width: 30rpx;
    margin-right: 20rpx;
    vertical-align: center;
}
.detail{
    padding-top: 10rpx;
    background-color: #FBF9FE;
}
.toleft{
    
    float:left;
    padding-left: 18rpx;
}
.toright{
    float:right;
    padding-right: 18rpx;
}
.timedetail{
    display: inline-block; 
    position: fiexd;
    bottom: 0;  
    padding-top: 10rpx;
    padding-left: 18rpx;
}
